{
  "name": "simple-service-status",
  "version": "1.3.1",
  "description": "A simple tool that supports management UI and query API for announcement service status designating specific version of app or platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run start:prod",
    "start:prod": "node dist/index.js",
    "start:dev": "gulp dev-start",
    "webpack:prod": "NODE_ENV=production webpack --config=./webpack.config.js",
    "webpack:dev": "NODE_ENV=development webpack --config=./webpack.config.js",
    "test": "jest --forceExit",
    "lint": "eslint ./src",
    "heroku-postbuild": "gulp server-build && npm run webpack:prod"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "clover"
    ]
  },
  "keywords": [
    "status"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ridi/simple-service-status.git"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.20.0",
    "babel-register": "^6.18.0",
    "co": "^4.6.0",
    "dotenv": "^4.0.0",
    "draft-js": "^0.10.0",
    "draft-js-export-html": "^0.6.0",
    "hapi": "^16.0.1",
    "hapi-auth-cookie": "^7.0.0",
    "hapi-auth-jwt2": "^7.2.4",
    "hapi-react-views": "^9.2.1",
    "inert": "^4.0.3",
    "joi": "^10.2.0",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.23.0",
    "mongodb": "^2.2.16",
    "node-cache": "^4.1.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-bootstrap": "^0.31.2",
    "react-datetime": "^2.8.1",
    "react-dom": "^15.4.1",
    "react-dom-factories": "^1.0.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-selectize": "^3.0.1",
    "semver": "^5.3.0",
    "vision": "^4.1.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "@ridi/eslint-config": "*",
    "babel-core": "^6.26.3",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.0",
    "express": "^4.15.2",
    "gulp": "^4.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.4",
    "h2o2": "^5.4.0",
    "jest-cli": "^20.0.4",
    "json-loader": "^0.5.4",
    "node-sass": "^4.11.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.18.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.11"
  }
