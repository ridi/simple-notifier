{
  "name": "simple-service-status",
  "version": "1.2.0",
  "engines": {
    "node": "6.x",
    "npm": "3.x",
    "yarn": "^0.27.5"
  },
  "description": "A simple tool that supports management UI and query API for announcement service status designating specific version of app or platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn run start:prod",
    "start:prod": "gulp server-build && yarn run webpack:prod && NODE_ENV=production node dist/index.js",
    "start:dev": "gulp dev-start",
    "webpack:prod": "NODE_ENV=production webpack --config=./webpack.config.js",
    "webpack:dev": "NODE_ENV=development webpack --config=./webpack.config.js",
    "test": "jest --forceExit"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "clover"
    ]
  },
  "keywords": [
    "status"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ridi/simple-service-status.git"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "co": "^4.6.0",
    "dotenv": "^4.0.0",
    "draft-js": "^0.10.0",
    "draft-js-export-html": "^0.6.0",
    "hapi": "^16.0.1",
    "hapi-auth-cookie": "^7.0.0",
    "hapi-auth-jwt2": "^7.2.4",
    "hapi-react-views": "^9.2.1",
    "inert": "^4.0.3",
    "joi": "^10.2.0",
    "jsonwebtoken": "^7.2.1",
    "moment": "^2.17.1",
    "mongodb": "^2.2.16",
    "node-cache": "^4.1.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-bootstrap": "^0.31.2",
    "react-datetime": "^2.8.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-selectize": "^3.0.1",
    "semver": "^5.3.0",
    "vision": "^4.1.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.0",
    "eslint-config-ridibooks": "*",
    "express": "^4.15.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.0",
    "h2o2": "^5.4.0",
    "jest-cli": "^20.0.4",
    "json-loader": "^0.5.4",
    "node-sass": "^4.1.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
